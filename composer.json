{
    "name": "aloisogomes/boletototal",
    "type": "library",
    "description": "Projeto para emissão de boletos, geração de arquivos de remessa e recepção de arquivos de retorno",
    "keywords": [
        "Boleto","PHP","Remessa","Retorno","Emissao"
    ],
    "authors": [
        {
            "name": "Aloiso Gomes",
            "email": "aloiso.gomes@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/aloisogomes/boletototal/issues"
    },
    "minimum-stability":"dev",
    "require": {
        "php": ">=5.3.0"

    },
    "require-dev":{
        "cobregratis/boletophp":"dev-2.x-dev"
    }
    "autoload": {
        "psr-4": {
            "BoletoTotal\\": "src/BoletoTotal/"
        }
    },
    "config": {
        "process-timeout": 1200
    }
